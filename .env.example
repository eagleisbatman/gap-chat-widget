# OpenAI API Key (required)
# Get this from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Agent Builder Workflow ID (required)
# Get this from https://platform.openai.com/agent-builder
WORKFLOW_ID=wf_your_workflow_id_here

# Farm Location Coordinates (required)
# Default location: Kenya (Nairobi region)
FARM_LATITUDE=-1.2864
FARM_LONGITUDE=36.8172

# Server Port (optional, defaults to 3002)
PORT=3002
